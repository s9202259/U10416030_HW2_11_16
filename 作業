import java.util.ArrayList;
import java.util.Scanner;

public class RepeatAdditionQuiz {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int number1 = (int)(Math.random()*10);
		int number2 = (int)(Math.random()*10);
		int inputNumber;
		ArrayList<String> alreadyEnter = new ArrayList<String>();
		
		System.out.println("what is " + number1 + " + " + number2 + " ?");
		inputNumber = input.nextInt();
		alreadyEnter.add("" +inputNumber);
		
		while (inputNumber != number1 + number2) {
			System.out.println("Wrong answer, please enter anain:");
			inputNumber = input.nextInt();
			alreadyEnter.add("" +inputNumber);
			while (alreadyEnter.contains("" +inputNumber)) {
				System.out.println("You already enter " + inputNumber 
						+ ", please enter again");
				inputNumber = input.nextInt();
				alreadyEnter.add("" +inputNumber);
			}
		}
		System.out.println("You got it!");
	}
}

